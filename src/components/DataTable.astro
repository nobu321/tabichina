---
export interface Props {
  headers: string[];
  rows: string[][];
}

const { headers, rows } = Astro.props;
---

<table class="data-table">
  <thead>
    <tr>
      {headers.map(header => (
        <th>{header}</th>
      ))}
    </tr>
  </thead>
  <tbody>
    {rows.map(row => (
      <tr>
        {row.map(cell => (
          <td set:html={cell}></td>
        ))}
      </tr>
    ))}
  </tbody>
</table>